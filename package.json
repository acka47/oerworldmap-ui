{
  "name": "oerworldmap-ui",
  "version": "0.0.1",
  "description": "The user interface for https://oerworldmap.org/",
  "scripts": {
    "clean": "rm -rf ./dist",
    "server:dev": "NODE_ENV=development node src/index.js",
    "server:static": "NODE_ENV=static node src/index.js",
    "server:prod": "npm run clean && NODE_ENV=production webpack && NODE_ENV=production node src/index.js",
    "test": "eslint src --quiet && mocha --require babel-core/register --require ignore-styles test/helpers/browser.js test/*.js",
    "fix": "eslint src --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src --ext .jsx,.js --quiet",
    "lint:style": "stylelint 'src/styles/**/*.pcss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbz/oerworldmap-ui.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hbz/oerworldmap-ui/issues"
  },
  "homepage": "https://github.com/hbz/oerworldmap-ui#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-stylelint": "^8.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jsdom": "^11.4.0",
    "mocha": "^4.0.1",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "d3": "^4.12.0",
    "d3-pie": "^1.1.0",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "getenv": "^0.7.0",
    "http-link-header": "^0.8.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-pointer": "^0.6.0",
    "json-pointer-form": "file:local_modules/json-pointer-form",
    "mapbox-gl": "^0.42.2",
    "mitt": "^1.1.3",
    "moment": "^2.20.1",
    "node-polyglot": "^2.2.2",
    "normalize.css": "^7.0.0",
    "path-to-regexp": "^2.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "prop-types": "^15.6.0",
